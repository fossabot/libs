{
  "groups": [
    {
      "name": "ihub.application",
      "description ": "服务相关配置"
    }
  ],
  "properties": [
    {
      "name": "ihub.application.name",
      "type": "java.lang.String",
      "defaultValue": "service",
      "description": "服务名称."
    },
    {
      "name": "ihub.application.port",
      "type": "java.lang.Integer",
      "defaultValue": 8080,
      "description": "服务端口."
    },
    {
      "name": "ihub.application.name.prefix",
      "type": "java.lang.String",
      "defaultValue": "ihub-",
      "description": "服务名称前缀."
    },
    {
      "name": "ihub.application.gateway-server-addr",
      "type": "java.lang.String",
      "defaultValue": "localhost",
      "description": "网关服务地址."
    }
  ]
}
